{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                u.id,\n                u.username,\n                u.email,\n                u.role_id,\n                COALESCE(u.is_active, false) as \"is_active!\",\n                r.role_name\n            FROM users u\n            JOIN roles r ON u.role_id = r.id\n            WHERE u.id = $1\n            ",
