{
  "db_name": "PostgreSQL",
  "query": "\n                    SELECT\n                        u.id,\n                        u.username,\n                        u.email,\n                        u.role_id,\n                        r.role_name,\n                        u.is_active,\n                        COUNT(*) OVER() as \"total_items\"\n                    FROM users u\n                    JOIN roles r ON u.role_id = r.id\n                    ORDER BY u.id ASC\n                    LIMIT $1 OFFSET $2\n                    ",
